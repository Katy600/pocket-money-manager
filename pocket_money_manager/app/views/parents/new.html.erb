<%= form_for(@parent) do
    |f| %>

<table>
  <tr>
    <th>First name</th>
    <td><%= f.text_field(:first_name) %></td>
  </tr>
  <tr>
    <th>Last name </th>
    <td><%= f.text_field(:last_name) %></td>
  </tr>
  <tr>
    <th>Email</th>
    <td><%= f.text_field(:email) %></td>
  </tr>
  <tr>
    <th>Password</th>
    <td><%= f.text_field(:password) %></td>
  </tr>
</table>

<%= f.submit("Submit") %>
<% end %>
